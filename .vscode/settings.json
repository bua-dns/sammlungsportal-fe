{
  "editor.formatOnSave": false,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.format.enable": true,
  "files.eol": "\n",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "cSpell.words": [
    "impressum",
    "Nuxt",
    "sammlungen",
    "Sammlungsportal",
    "sammlungstagung",
    "sitenav"
  ]
}